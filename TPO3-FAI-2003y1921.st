Object subclass: #Agencia	instanceVariableNames: 'nombre colVehiculos colAlquileres colPreciosCamion colPreciosAdicional'	classVariableNames: ''	poolDictionaries: ''	category: 'TPO3-FAI-2003y1921'!!Agencia methodsFor: 'as yet unclassified' stamp: 'FAI2003y1921 6/6/2019 11:13'!agregarVehiculo: unVehiculocolVehiculos add: unVehiculo .! !!Agencia methodsFor: 'as yet unclassified' stamp: 'FAI2003y1921 6/6/2019 11:14'!agregarPrecioCamion: unPrecioCamcolPreciosCamion add: unPrecioCam .! !!Agencia methodsFor: 'as yet unclassified' stamp: 'FAI2003y1921 6/6/2019 11:13'!agregarPrecioAdicional: unPreciocolPreciosAdicional add: unPrecio.! !!Agencia methodsFor: 'as yet unclassified' stamp: 'FAI2003y1921 6/6/2019 10:47'!inicializarAgencia: unNombre	"Para inicializar una agencia"	colAlquileres := OrderedCollection new.	colPreciosAdicional := OrderedCollection new.	colVehiculos := OrderedCollection new.	colPreciosCamion := OrderedCollection new.	nombre := unNombre.! !!Agencia methodsFor: 'as yet unclassified' stamp: 'LeandroVallejos 6/7/2019 13:49'!buscarValor: unAdicional "para los adicionales de la clase Auto"|precioAdicional|precioAdicional := 0.colPreciosAdicional do:[:cadaAdicional| [(cadaAdicional at: 1) = unAdicional]value ifTrue:[precioAdicional := (cadaAdicional at: 2)]].^precioAdicional.! !!Agencia methodsFor: 'as yet unclassified' stamp: 'FAI2003y1921 6/6/2019 11:12'!agregarAlquiler: unAlquiler	colAlquileres add: unAlquiler.! !!Agencia methodsFor: 'as yet unclassified' stamp: 'LeandroVallejos 6/7/2019 13:24'!calcularTipo: unTipo|ret|ret := 0.colPreciosCamion do:[:cadaPrecio| [(cadaPrecio at: 1) = unTipo]value ifTrue:[ret := (cadaPrecio at: 2)]].^ret.! !!Agencia methodsFor: 'accessing' stamp: 'FAI2003y1921 6/6/2019 11:05'!nombre	^ nombre! !!Agencia methodsFor: 'accessing' stamp: 'FAI2003y1921 6/6/2019 11:05'!colPreciosAdicional	^ colPreciosAdicional! !!Agencia methodsFor: 'accessing' stamp: 'FAI2003y1921 6/6/2019 11:05'!colVehiculos	^ colVehiculos! !!Agencia methodsFor: 'accessing' stamp: 'FAI2003y1921 6/6/2019 11:05'!colPreciosCamion	^ colPreciosCamion! !!Agencia methodsFor: 'accessing' stamp: 'FAI2003y1921 6/6/2019 11:05'!colAlquileres	^ colAlquileres! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Agencia class	instanceVariableNames: ''!!Agencia class methodsFor: 'as yet unclassified' stamp: 'FAI2003y1921 6/6/2019 10:48'!crearAgencia: unNombreAgencia	"Constructor de agencia"|agenc|agenc := self new.agenc inicializarAgencia: unNombreAgencia.^agenc.! !Object subclass: #Alquiler	instanceVariableNames: 'cliente fechaIni vehiculo valorFinal'	classVariableNames: ''	poolDictionaries: ''	category: 'TPO3-FAI-2003y1921'!!Alquiler methodsFor: 'as yet unclassified' stamp: 'LeandroVallejos 6/6/2019 23:34'!devolverVehiculo: laAgencia kmRecorridos: km	"mensaje para devolverle un vehiculo a la agencia"	|valor|	vehiculo actualizarDatos: km.	valor := self precioFinal: laAgencia kilometros: km.	^valor.! !!Alquiler methodsFor: 'as yet unclassified' stamp: 'FAI2003y1921 6/6/2019 11:08'!inicializarAlquiler: unCliente conFecha: unaFecha deVehiculo: unVehiculo	"Para inicializar un alquiler"	cliente := unCliente.	fechaIni := unaFecha.	vehiculo := unVehiculo.	valorFinal := 0.! !!Alquiler methodsFor: 'accessing' stamp: 'FAI2003y1921 6/6/2019 11:06'!fechaIni	^ fechaIni! !!Alquiler methodsFor: 'accessing' stamp: 'FAI2003y1921 6/6/2019 11:06'!vehiculo	^ vehiculo! !!Alquiler methodsFor: 'accessing' stamp: 'FAI2003y1921 6/6/2019 11:06'!valorFinal	^ valorFinal! !!Alquiler methodsFor: 'accessing' stamp: 'LeandroVallejos 6/6/2019 23:33'!precioFinal: arg1 kilometros: arg2 	^ self subclassResponsibility! !!Alquiler methodsFor: 'accessing' stamp: 'FAI2003y1921 6/6/2019 11:06'!cliente	^ cliente! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Alquiler class	instanceVariableNames: ''!!Alquiler class methodsFor: 'as yet unclassified' stamp: 'LeandroVallejos 6/6/2019 23:10'!crearAlquiler: unCliente deFecha: unaFecha deVehiculo: unVehi 	"comment stating purpose of message"	|alq|	alq := self new.	alq inicializarAlquiler: unCliente  conFecha: unaFecha  deVehiculo: unVehi.	^alq. ! !Alquiler subclass: #Alq_Camiones	instanceVariableNames: 'elemATransportar dias'	classVariableNames: ''	poolDictionaries: ''	category: 'TPO3-FAI-2003y1921'!!Alq_Camiones methodsFor: 'as yet unclassified' stamp: 'LeandroVallejos 6/7/2019 12:59'!precioFinal: unaAgencia kilometros: kmrecorridos|val tipo carga|val := 0.tipo := vehiculo tipoCamion.carga := vehiculo tipoCarga.val := [(Date today subtractDate: fechaIni) * (unaAgencia calcularTipo: tipo)]value.	[carga == 'semipesado']value ifTrue: [val := val + ((val * 100)/25)].^val.	! !!Alq_Camiones methodsFor: 'accessing' stamp: 'FAI2003y1921 6/6/2019 11:16'!dias	^ dias! !!Alq_Camiones methodsFor: 'accessing' stamp: 'FAI2003y1921 6/6/2019 11:47'!inicializarAlquilerCamiones: unCliente deFecha: unaFecha deCamion: unCamion	self inicializarAlquiler: unCliente conFecha: unaFecha deVehiculo: unCamion. 	dias := 0.	elemATransportar := nil.! !!Alq_Camiones methodsFor: 'accessing' stamp: 'FAI2003y1921 6/6/2019 11:16'!elemATransportar	^ elemATransportar! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Alq_Camiones class	instanceVariableNames: ''!!Alq_Camiones class methodsFor: 'as yet unclassified' stamp: 'FAI2003y1921 6/6/2019 11:44'!crearAlquilerCamiones: unCliente deFecha: unaFecha deCamion: unCamion   	"comment stating purpose of message"	|alqCam|	alqCam := self new.	alqCam inicializarAlquilerCamiones: unCliente  deFecha: unaFecha  deCamion: unCamion.	^alqCam. ! !Alquiler subclass: #Alq_Turismo	instanceVariableNames: 'colAdicionales'	classVariableNames: ''	poolDictionaries: ''	category: 'TPO3-FAI-2003y1921'!!Alq_Turismo methodsFor: 'accessing' stamp: 'FAI2003y1921 6/6/2019 11:15'!colAdicionales	^ colAdicionales! !!Alq_Turismo methodsFor: 'as yet unclassified' stamp: 'LeandroVallejos 6/7/2019 13:26'!agregarAdicional: unAdiccolAdicionales add: unAdic.! !!Alq_Turismo methodsFor: 'as yet unclassified' stamp: 'LeandroVallejos 6/7/2019 13:49'!precioFinal: unaAgencia kilometros: kmrecorridos|val pAdicional|pAdicional := 0.val := vehiculo precioPorKilometro: kmrecorridos.colAdicionales do:[:cadaAdicional| pAdicional := (unaAgencia buscarValor: cadaAdicional)].^ (val + pAdicional).! !!Alq_Turismo methodsFor: 'as yet unclassified' stamp: 'FAI2003y1921 6/6/2019 11:52'!inicializarAlquilerTurismo: unCliente deFecha: unaFecha deAuto: unAutoself inicializarAlquiler: unCliente conFecha: unaFecha deVehiculo: unAuto.colAdicionales := OrderedCollection new. ! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Alq_Turismo class	instanceVariableNames: ''!!Alq_Turismo class methodsFor: 'as yet unclassified' stamp: 'LeandroVallejos 6/7/2019 12:24'!crearAlquilerTurismo: clien deFecha: dat deAuto: auto|alqTur|	alqTur := self new.	alqTur inicializarAlquilerTurismo: clien deFecha: dat deAuto: auto.	^alqTur. ! !Object subclass: #Cliente	instanceVariableNames: 'dni nombreYApe tarjeta'	classVariableNames: ''	poolDictionaries: ''	category: 'TPO3-FAI-2003y1921'!!Cliente methodsFor: 'accessing' stamp: 'FAI2003y1921 6/6/2019 11:08'!nombreYApe	^ nombreYApe! !!Cliente methodsFor: 'accessing' stamp: 'FAI2003y1921 6/6/2019 11:08'!tarjeta: anObject	tarjeta := anObject! !!Cliente methodsFor: 'accessing' stamp: 'FAI2003y1921 6/6/2019 11:08'!tarjeta	^ tarjeta! !!Cliente methodsFor: 'accessing' stamp: 'FAI2003y1921 6/6/2019 11:08'!dni	^ dni! !!Cliente methodsFor: 'as yet unclassified' stamp: 'FAI2003y1921 6/6/2019 10:57'!inicializarCliente: unNombreYApellido dni: unDNI	"Inicializador de cliente"	nombreYApe := unNombreYApellido.	dni := unDNI.	tarjeta := ''.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Cliente class	instanceVariableNames: ''!!Cliente class methodsFor: 'as yet unclassified' stamp: 'FAI2003y1921 6/6/2019 10:55'!crearCliente: unNombreyApe conDNI: unDNI	"comment stating purpose of message"	|clien|	clien := self new.	clien inicializarCliente: unNombreyApe dni: unDNI.	^clien.! !Object subclass: #Vehiculo	instanceVariableNames: 'matricula marca modelo dispParaAlquilar kilometraje'	classVariableNames: ''	poolDictionaries: ''	category: 'TPO3-FAI-2003y1921'!!Vehiculo methodsFor: 'as yet unclassified' stamp: 'FAI2003y1921 6/6/2019 13:05'!inicializarVehiculo: unaMarca modelo: unModelo matricula: unaMatricula	"Inicializador de vehiculo"	marca := unaMarca.matricula := unaMatricula .modelo := unModelo.dispParaAlquilar := true.kilometraje := 0.! !!Vehiculo methodsFor: 'as yet unclassified' stamp: 'FAI2003y1921 6/6/2019 12:57'!actualizarDatos: unKM	"actualizar kilometraje de un vehiculo"	kilometraje := kilometraje + unKM.! !!Vehiculo methodsFor: 'accessing' stamp: 'FAI2003y1921 6/6/2019 10:31'!modelo	^ modelo! !!Vehiculo methodsFor: 'accessing' stamp: 'FAI2003y1921 6/6/2019 10:31'!marca	^ marca! !!Vehiculo methodsFor: 'accessing' stamp: 'FAI2003y1921 6/6/2019 10:31'!matricula	^ matricula! !!Vehiculo methodsFor: 'accessing' stamp: 'FAI2003y1921 6/6/2019 10:31'!kilometraje	^ kilometraje! !!Vehiculo methodsFor: 'accessing' stamp: 'FAI2003y1921 6/6/2019 13:02'!dispParaAlquilar	^ dispParaAlquilar! !!Vehiculo methodsFor: 'accessing' stamp: 'FAI2003y1921 6/6/2019 10:31'!kilometraje: anObject	kilometraje := anObject! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Vehiculo class	instanceVariableNames: ''!!Vehiculo class methodsFor: 'as yet unclassified' stamp: 'FAI2003y1921 6/6/2019 11:02'!crearVehiculo: unaMarca modelo: unModelo matricula: unaMat	"Constructor de vehiculo"	|vehi|	vehi := self new.	vehi inicializarVehiculo: unaMarca modelo: unModelo  matricula: unaMat .	^vehi. ! !Vehiculo subclass: #Auto	instanceVariableNames: 'gps tarifa'	classVariableNames: ''	poolDictionaries: ''	category: 'TPO3-FAI-2003y1921'!!Auto methodsFor: 'as yet unclassified' stamp: 'FAI2003y1921 6/6/2019 11:55'!inicializarAuto: unaMarca modelo: unModelo matricula: unaMatricula conTarifa: unaTarifa.	"Inicializador de auto"self inicializarVehiculo: unaMarca modelo: unModelo matricula: unaMatricula.gps := false.tarifa := unaTarifa.! !!Auto methodsFor: 'as yet unclassified' stamp: 'LeandroVallejos 6/6/2019 23:03'!precioPorKilometro: unosKM|dev|dev := unosKM * tarifa. "tarifa es el precio por kilometro del auto"^dev.! !!Auto methodsFor: 'accessing' stamp: 'FAI2003y1921 6/6/2019 11:16'!gps	^ gps! !!Auto methodsFor: 'accessing' stamp: 'FAI2003y1921 6/6/2019 11:16'!tarifa	^ tarifa! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Auto class	instanceVariableNames: ''!!Auto class methodsFor: 'as yet unclassified' stamp: 'FAI2003y1921 6/6/2019 11:58'!crearAuto: unaMarca modelo: unModelo matricula: unaMat tarifa: unaTarifa|autito|	autito := self new.	autito inicializarAuto: unaMarca modelo: unModelo  matricula: unaMat  conTarifa: unaTarifa .	^autito. ! !Vehiculo subclass: #Camion	instanceVariableNames: 'tipoCarga tipoCamion'	classVariableNames: ''	poolDictionaries: ''	category: 'TPO3-FAI-2003y1921'!!Camion methodsFor: 'accessing' stamp: 'FAI2003y1921 6/6/2019 11:16'!tipoCarga	^ tipoCarga! !!Camion methodsFor: 'accessing' stamp: 'FAI2003y1921 6/6/2019 11:16'!tipoCamion	^ tipoCamion! !!Camion methodsFor: 'as yet unclassified' stamp: 'FAI2003y1921 6/6/2019 13:05'!verificarService	|bool|  bool := [kilometraje > 80000]value.bool ifTrue: [dispParaAlquilar := false].! !!Camion methodsFor: 'as yet unclassified' stamp: 'FAI2003y1921 6/6/2019 12:02'!inicializarCamion: unaMarca modelo: unModelo matricula: unaMat ligeroOsemi: unaCarga tipo: unTipo	"Inicializador de camion"self inicializarVehiculo: unaMarca modelo: unModelo matricula: unaMat.tipoCamion := unTipo.tipoCarga := unaCarga.! !!Camion methodsFor: 'as yet unclassified' stamp: 'FAI2003y1921 6/6/2019 13:01'!actualizarDatos: unKM	"actualizar kilometraje de un camion, igual que su padre pero llamando al metodo verificarService(kilometraje)"	super actualizarDatos: unKM.	self verificarService.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Camion class	instanceVariableNames: ''!!Camion class methodsFor: 'as yet unclassified' stamp: 'FAI2003y1921 6/6/2019 12:04'!crearCamion: unaMarca modelo: unModelo matricula: unaMat ligero_Semi: unaCarga tipoCamion: unTipo|camioncito|	camioncito := self new.	camioncito inicializarCamion: unaMarca modelo: unModelo matricula: unaMat ligeroOsemi: unaCarga tipo: unTipo.	^camioncito. ! !